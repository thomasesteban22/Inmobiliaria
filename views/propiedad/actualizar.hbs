<form method="post" action="/propiedad/actualizar" class="form-horizontal">
<input type="hidden" value="{{propiedad.id}}" name="id">
  <div class="form-group">
    <label  class="col-sm-2 control-label">Nombre</label>
    <div class="col-sm-10">
        <select name="nombre" class="form-control" value="{{propiedad.nombre}}" >
            <option value="Casa en venta" >Casa en venta</option>
             <option value="Apartamento en venta" >Apartamento en venta</option>
              <option value="Local en venta" >Local en venta</option>
               <option value="Casa en arriendo" >Casa en arriendo</option>
                <option value="Apartamento en arriendo" >Apartamento en arriendo</option>
                 <option value="Local en arriendo" >Local en arriendo</option>
        </select>
    </div>
  </div>
  
  <div class="form-group">
    <label  class="col-sm-2 control-label">NumeroDeBa√±os</label>
    <div class="col-sm-10">
     <select name="nbanos" class="form-control"  value="{{propiedad.nbanos}}">
         <option value="0">0</option>
         <option value="1">1</option>
        <option value="2">2</option>
         <option value="3">3</option>
         <option value="4">4</option>
         <option value="5">5</option>
         <option value="6">6</option>
         <option value="7">7</option>
         <option value="8">8</option>
         <option value="9">9</option>    
     </select>
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">NumeroDeCuartos</label>
    <div class="col-sm-10">
      <select name="ncuartos" class="form-control" value="{{propiedad.ncuartos}}">
         <option value="0">0</option>
         <option value="1">1</option>
        <option value="2">2</option>
         <option value="3">3</option>
         <option value="4">4</option>
         <option value="5">5</option>
         <option value="6">6</option>
         <option value="7">7</option>
         <option value="8">8</option>
         <option value="9">9</option>    
     </select>
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">NumeroDeGarages</label>
    <div class="col-sm-10">
       <select name="ngarajes" class="form-control"  value="{{propiedad.ngarajes}}">
         <option value="0">0</option>
         <option value="1">1</option>
        <option value="2">2</option>
         <option value="3">3</option>
         <option value="4">4</option>
         <option value="5">5</option>
         <option value="6">6</option>
         <option value="7">7</option>
         <option value="8">8</option>
         <option value="9">9</option>    
     </select>
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">MetrosCuadrados</label>
    <div class="col-sm-10">
      <input name="metrosCuadrados" class="form-control"  value="{{propiedad.metrosCuadrados}}">
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">TipoDePropiedad</label>
    <div class="col-sm-10">
       <select name="tipoDePropiedad" class="form-control" value="{{propiedad.tipoDePropiedad}}">
            <option value="Casa" >Casa</option>
             <option value="Apartamento" >Apartamento</option>
              <option value="Local" >Local</option>
        </select>
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">TipoDeInmueble</label>
    <div class="col-sm-10">
      <select name="tipoDeInmueble" class="form-control"  value="{{propiedad.tipoDeInmueble}}">
            <option value="Nuevo" >Nuevo</option>
             <option value="Usado" >Usado</option>
        </select>
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">Precio</label>
    <div class="col-sm-10">
      <input name="precio" class="form-control"  value="{{propiedad.precio}}" >
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">Barrio</label>
    <div class="col-sm-10">
      <input name="barrio" class="form-control"  value="{{propiedad.barrio}}" >
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">Direccion</label>
    <div class="col-sm-10">
      <input name="direccion" class="form-control"  value="{{propiedad.direccion}}" >
    </div>
  </div>

  <div class="form-group">
    <label  class="col-sm-2 control-label">Descripcion</label>
    <div class="col-sm-10">
      <input name="descripcion" class="form-control"  value="{{propiedad.descripcion}}" >
    </div>
  </div>

  
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">GuardarPropiedad</button>
    </div>
  </div>
</form>