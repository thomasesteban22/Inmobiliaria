<form action="/auth/login" method="post">
    <h3>Login</h3>
    <input type="email" name="email" placeholder="ESCRIBA SU EMAIL">
    <input type="password" name="password" placeholder="ESCRIBA SU PASSWORD">
    <button type="submit">Conectar</button>
    <a href="/auth/google"><i class="fa fa-google-plus-square" aria-hidden="true"></i></a>
    <a href="/auth/facebook"><i class="fa fa-facebook-official" aria-hidden="true"></i></a>
</form>