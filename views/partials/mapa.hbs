   <style>
      #map {
        width: 100%;
        height: 400px;
        background-color: grey;
      }
    </style>

    <div id="map"></div>
    <script>
      function initMap() {
        var uluru = {lat:  4.679155, lng: -74.058996};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 20,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBp4uG0V6hHu1FeBfllS7322ykwp6srYI8&callback=initMap">
    </script>

