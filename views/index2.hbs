<div class="row">
    <!--Title-->
    <p class="titulo1">Busca tu propiedad</p>
    <!--End title-->
    <div class="row">
        <div class="col-sm-12">
            <form method="post" class="form-inline navbar-form" action="/">
                <nav class=" navbar-default nav2">
                    <div class="">
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav">
                                <li class="dropdown dropdownstyle">
                                    <select name="tipoDeInmueble" class="form-control">
                        <option value="">Tipo de Inmueble</option>
                        {{#each tipoDeInmuebles}}
                        <option value="{{valor}}">{{valor}}</option>
                        {{/each}}
                    </select>
                                </li>
                            </ul>

                            <ul class="nav navbar-nav">
                                <li class="dropdown dropdownstyle">
                                    <select name="ngarajes" class="form-control">
                        <option value="" >Garajes</option>
                        {{#each garajes}}
                        <option value="{{cantidad}}">{{cantidad}}</option>
                        {{/each}}
                    </select>
                                </li>
                            </ul>
                            <ul class="nav navbar-nav">
                                <li>
                                    <select name="tipoDePropiedad" class="form-control">
                    <option value="" >Tipo de propiedad</option>
                    {{#each tipoDePropiedades}}
                    <option value={{valor}}>{{valor}}</option>
                    {{/each}}
                </select>
                                </li>
                            </ul>
                            <ul class="nav navbar-nav">
                                <li class="dropdown dropdownstyle">
                                    <select name="ncuartos" class="form-control">
                        <option value="" >Seleccione cuartos</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>

                    </select>
                                </li>
                            </ul>
                            <ul class="nav navbar-nav">
                                <li class="dropdown dropdownstyle">
                                    <select name="nbanos" class="form-control">
                        <option value="" >Seleccionar baños</option>
                        {{#each banos}}
                        <option value="{{cantidad2}}">{{cantidad2}}</option>
                        {{/each}}
                    </select>
                                </li>
                            </ul>
                            <ul class="nav navbar-nav">
                                <li class="dropdown dropdownstyle">
                                    <button type="submit" class="btn btn-default">Buscar</button>
                                </li>
                            </ul>

                        </div>

                    </div>
                    <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
        </nav>
        </form>
    </div>
</div>


<!--AQUI COMIENZA EL SLIDER-->
<div class="row">
    <h1 class="Subtitulo">Ultimas Propiedades</h1>

    {{#each propiedades}}
    <div class="col-sm-3">
        <div id="{{@index}}" class="mi-slider carousel slide" data-ride="carousel">
            <!-- Wrapper for slides -->
            <div class="carousel-inner" role="listbox">
                {{#each imagenes}}
                <div class="item {{clase}}">
                    <img src="{{path}}" alt="..." height="100%" width="100%">
                    <div class="carousel-caption">
                    </div>
                </div>
                {{/each}}
                <!-- Controls -->
                <a class="left carousel-control" href="#{{@index}}" role="button" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
                <a class="right carousel-control" href="#{{@index}}" role="button" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
            </div>
        </div>
        {{/each}}
        <!--AQUI TERMINA EL SLIDERRRR-->

    </div>
</div>






